{
  "name": "api",
  "version": "0.0.1",
  "description": "",
  "main": "src/server.ts",
  "scripts": {
    "dev": "nodemon",
    "build": "rimraf ./build && tsc",
    "start": "npm run build && node build/src/server.js",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli ",
    "migrate:create": "typeorm migration:create \"./database/migrations/",
    "migrate:up": "ts-node --transpile-only ./node_modules/typeorm/cli.js migration:run -d data-source.ts",
    "migrate:down": "ts-node --transpile-only ./node-modules/typeorm/cli.js migration:revert"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "better-sqlite3": "^7.6.2",
    "cors": "^2.8.5",
    "cpf-cnpj-validator": "^1.0.3",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "heroku": "^7.60.2",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.0.9",
    "tsconfig-paths": "^4.0.0",
    "typeorm": "^0.3.7",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^18.0.6",
    "nodemon": "^2.0.19",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
